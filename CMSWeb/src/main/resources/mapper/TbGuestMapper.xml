  <!-- guest click in/out report -->
  <select id="findClickInOutRecord"  parameterType="com.note.cms.data.vo.GuestReportParams" resultMap="GuestClickInOutResultMap">
    SELECT g.id , g.card_no as code , g.name , MIN(s.create_time) as click_in , MAX(s.create_time) as click_out from tb_guest g , tb_snapshot s
    where g.guest_role_id = #{guestRoleId} and g.id = s.guest_code
    and s.create_time between #{startTime} and #{endTime}
    group by g.id , g.code , g.name
  </select>

  <select id="findClickInOutRecordByIpc"  parameterType="com.note.cms.data.vo.GuestReportParams" resultMap="GuestClickInOutResultMap">
    SELECT g.id , g.card_no as code , g.name , MIN(s.create_time) as click_in , MAX(s.create_time) as click_out from tb_guest g , tb_snapshot s
    where s.ipc_id=#{ipcId} and g.guest_role_id = #{guestRoleId} and g.id = s.guest_code
    and s.create_time between #{startTime} and #{endTime}
    group by g.id , g.code , g.name
  </select>

